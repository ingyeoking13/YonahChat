<Window
    x:Class="YonahChat.WPF.Main.Views.MainWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:core="clr-namespace:YonahChat.WPF.Core;assembly=YonahChat.WPF.Core"
    xmlns:prism="http://prismlibrary.com/"
    Title="{Binding Title}"
    Width="860"
    Height="550"
    prism:ViewModelLocator.AutoWireViewModel="True"
    WindowStartupLocation="CenterScreen">
    <Grid>
        <ContentControl prism:RegionManager.RegionName="{x:Static core:RegionNames.RootContentRegion}" />
    </Grid>
</Window>
